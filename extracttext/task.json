{
    "id": "FLOWS/pdf/extracttext",
    "lambda": "lambda-arn",
    "role": "iam-arn",
    "input": {
        "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "description": "pdf file",
            "type": "object",
            "properties": {
                "account": {
                    "type": "string"
                },
                "bucketName": {
                    "type": "string"
                },
                "objectName": {
                      "type": "string"
                  }
            },
            "required": ["account"]
        },
        "examples": {
            "pdffile": {
              "file":"https://blah.com/1.pdf"
            }
        }
    },
    "output": {
        "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "description": "to be defined",
            "type": "object"
        },
        "examples": {
            "result": {
                "name": "value"
            }
        }
    },
    "type": "task",
    "description": "extract all text data from a pdf (does not OCR images)",
    "icon": "https://flows.app/favicon.png",
    "billing": "standard"
}
